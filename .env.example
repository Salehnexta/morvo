# Enhanced Environment Variables for Morvo AI with MCP & A2A Integration
# متغيرات البيئة المحسنة لـ Morvo AI مع تكامل بروتوكولات MCP و A2A

# ===== CORE REQUIREMENTS =====
# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Server Configuration  
PORT=8001
HOST=0.0.0.0
LOG_LEVEL=INFO
DEBUG=false

# App Metadata
APP_VERSION=2.0.0
APP_NAME=Morvo AI Enhanced
APP_DESCRIPTION="Enhanced AI Marketing Platform with MCP and A2A Protocols"

# CORS Configuration
CORS_ORIGINS=["http://localhost:3000", "https://your-frontend-domain.com"]

# ===== DATABASE & STORAGE =====
# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# PostgreSQL Database
DATABASE_URL=postgresql://user:password@localhost:5432/morvo_ai
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=morvo_ai
DATABASE_USER=postgres
DATABASE_PASSWORD=your_db_password

# Redis Cache & Sessions
REDIS_URL=redis://localhost:6379/0
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password
REDIS_DB=0

# ===== SECURITY & AUTHENTICATION =====
# JWT Configuration
JWT_SECRET_KEY=morvo-ai-enhanced-secret-key-2025-change-this-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# OAuth Configuration
OAUTH_CLIENT_ID=your_oauth_client_id
OAUTH_CLIENT_SECRET=your_oauth_client_secret

# Encryption
ENCRYPTION_KEY=your_encryption_key_here

# ===== MCP (Model Context Protocol) CONFIGURATION =====
# MCP Server Settings
MCP_SERVER_HOST=localhost
MCP_SERVER_PORT=3000
MCP_TRANSPORT=streamable-http
MCP_MAX_CONNECTIONS=100
MCP_TIMEOUT=30

# ===== A2A (Agent-to-Agent) PROTOCOL CONFIGURATION =====
# A2A Network Settings
A2A_NETWORK_ID=morvo-ai-network
A2A_AUTH_ENABLED=true
A2A_MESSAGE_TTL=3600
A2A_MAX_RETRIES=3
A2A_RETRY_DELAY=5

# ===== GIT INTEGRATION =====
# Git Configuration
GIT_REPO_PATH=.
GIT_USERNAME=your_git_username
GIT_EMAIL=your_git_email
GIT_TOKEN=your_git_token
GIT_REMOTE_URL=https://github.com/your-username/morvo.git
GIT_AUTO_COMMIT=false
GIT_AUTO_PUSH=false

# ===== FILE PROCESSING =====
# Upload Configuration
UPLOAD_DIRECTORY=./uploads
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=pdf,txt,md,docx,png,jpg,jpeg

# ===== PERFORMANCE & SCALING =====
# Performance Settings
MAX_WORKERS=4
CACHE_TTL=3600
REQUEST_TIMEOUT=30
RATE_LIMIT_PER_MINUTE=60

# ===== MARKETING APIS =====
# SE Ranking API (Business Plan - $378/month)
SERANKING_API_KEY=your_seranking_api_key

# Awario API (Enterprise Plan - $399/month) 
AWARIO_API_KEY=your_awario_api_key
AWARIO_WEBHOOK_URL=https://your-domain.com/webhooks/awario

# Mention API (Pro Plus Plan - $179/month)
MENTION_API_KEY=your_mention_api_key
MENTION_ACCOUNT_ID=your_mention_account_id

# ===== SOCIAL MEDIA APIS =====
# Facebook & Instagram
FACEBOOK_ACCESS_TOKEN=your_facebook_access_token
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token

# Twitter/X
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_ACCESS_TOKEN=your_twitter_access_token
TWITTER_ACCESS_TOKEN_SECRET=your_twitter_access_token_secret

# LinkedIn
LINKEDIN_ACCESS_TOKEN=your_linkedin_access_token

# ===== ANALYTICS APIS =====
# Google Analytics
GOOGLE_ANALYTICS_CREDENTIALS=path/to/your/google-analytics-credentials.json
GOOGLE_ANALYTICS_PROPERTY_ID=your_ga_property_id

# SEO Tools
SEMRUSH_API_KEY=your_semrush_api_key
AHREFS_API_KEY=your_ahrefs_api_key

# ===== FEATURE FLAGS =====
# Protocol Features
MCP_PROTOCOL_ENABLED=true
A2A_PROTOCOL_ENABLED=true
SUPABASE_INTEGRATION_ENABLED=true
GIT_INTEGRATION_ENABLED=true
ADVANCED_CACHING_ENABLED=true
REAL_TIME_SYNC_ENABLED=true
WEBHOOK_SUPPORT_ENABLED=true
FILE_PROCESSING_ENABLED=true

# API Features
SOCIAL_MEDIA_APIS_ENABLED=true
ANALYTICS_APIS_ENABLED=true
PREDICTIVE_MODELING_ENABLED=true
AUTO_SCALING_ENABLED=true
SECURITY_MONITORING_ENABLED=true
PERFORMANCE_OPTIMIZATION_ENABLED=true

# ===== RAILWAY DEPLOYMENT =====
# Railway specific environment variables
RAILWAY_ENVIRONMENT=production
RAILWAY_PROJECT_ID=your_railway_project_id
RAILWAY_SERVICE_ID=your_railway_service_id

# ===== OPTIONAL DEVELOPMENT SETTINGS =====
# Development Only
DEV_RELOAD=true
DEV_LOG_SQL=false
DEV_PROFILING=false
DEV_MOCK_APIS=false

# Testing
TEST_DATABASE_URL=postgresql://user:password@localhost:5432/morvo_ai_test
TEST_REDIS_URL=redis://localhost:6379/1

# ===== NOTES =====
# 1. Change all "your_*" placeholders with actual values
# 2. Keep JWT_SECRET_KEY and ENCRYPTION_KEY secure and unique
# 3. Use environment-specific values for production
# 4. Marketing APIs will be activated according to subscription schedule
# 5. Social Media APIs require proper app registration and permissions
# 6. Git integration is optional but recommended for version control
# 7. MCP and A2A protocols enable advanced AI agent communication
# 8. Supabase provides real-time database and authentication
# 9. Redis improves performance through caching and session management
# 10. Enable features based on your deployment requirements
